<metal:block use-macro="main_template">
    <metal:content fill-slot="content">

    <header>
        <h1>Baltimore Office</h1>
        ${panel('popper.actions_menu')}
    </header>
    <div class="contentText">
        <div class="vcard clearfix">
            <div class="adr">
                <div class="street-address">10 North South Street,
                    Suite 100</div>
                <span class="locality">Anywhere</span>
                <abbr class="region">Missouri</abbr>
                <span class="postal-code">22222</span>
                <div class="country-name">United States</div>
            </div>
            <aside>
                <h3 class="fn org url hidden"
                    href="http://www.some.org">Popper-Thing</h3>
                <div class="tel"><b class="type">Tel</b>: 555-555-5555</div>
                <div class="tel"><b class="type">Fax</b>: 444-444-4444 </div>
                <b>Website</b>: <a class="url" href="http://www.some.org">http://www.some.org</a>
            </aside>
        </div>

        <div class="notification info">If you are calling from the Rome,
        Milan or Venice offices, dial directly by
        extension. If you are calling from the Miami office, dial
        #9, wait for a dial tone, then the extension.</div>

        ${panel('popper.grid_header', letters=letters, actions=actions, formats=formats)}

        <table class="filtered sortable" id="someid" summary="">
            <!-- 
                .filtered means the table is filtered and has filter bar above (used for a specific styling for this case)
                .sortable indicates that the table rows are sortable by clicking a column's header
             -->
            <thead>
                <tr>
                    <!-- Anchors within TH are required in .sortable -->
                    <th><a href="#" class="selected ascending">Name</a></th>
                    <th><a href="#">Department</a></th>
                    <th><a href="#">Position</a></th>
                    <th><a href="#">Phone</a></th>
                </tr>
            </thead>
            <tbody>
                <tr class="odd">
                    <td>
                        <div>
                            <a href="https://karl.some.org/profiles/billy/">Bill Budd</a>
                        </div>
                    </td>
                    <td >
                        <div>
                            UN Programs
                        </div>
                    </td>
                    <td>
                        <div>
                            Senior Development Associate
                        </div>
                    </td>
                    <td>
                        <div>
                            444-444-4444 x 4444
                        </div>
                    </td>
                </tr>
                <tr class="ui-grid-row even">
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 211px;">
                        <div>
                            <a href="https://karl.some.org/profiles/tcordes/">Tommy Person</a>
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 211px;">
                        <div>
                            UN Programs
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 212px;">
                        <div>
                            Program Associate - All Communications
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 212px;">
                        <div>
                            444-444-4444 x 4444
                        </div>
                    </td>
                </tr>
                <tr class="ui-grid-row odd ui-grid-row-hover">
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 211px;">
                        <div>
                            <a
                                    href="https://karl.some.org/profiles/mdixon/">Donna Dixon</a>
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 211px;">
                        <div>
                            Criminal Investigation
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 212px;">
                        <div>
                            Deputy Director
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 212px;">
                        <div>
                            444-444-4444 x 4444
                        </div>
                    </td>
                </tr>
                <tr class="ui-grid-row even">
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 211px;">
                        <div>
                            <a
                                    href="https://karl.some.org/profiles/kedwards/">Karen Karr</a>
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 211px;">
                        <div>
                            UN Programs
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 212px;">
                        <div>
                            Program Associate - Some Really Long Job
                            Title
                        </div>
                    </td>
                    <td style="overflow-x: hidden; overflow-y: hidden; width: 212px;">
                        <div>
                            444-444-4444 x 4444
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>

        ${panel('popper.grid_footer', batch=batch)}

    </div>
    </metal:content>
</metal:block>
